! to match gnome-terminal "Linux console" scheme
! foreground/background
URxvt.foreground: #C8C8C8
URxvt.background: #323232
URxvt.color0: #2E3436
URxvt.color1: #CC0000
URxvt.color2: #4E9A06
URxvt.color3: #C4A000
URxvt.color4: #3465A4
URxvt.color5: #75507B
URxvt.color6: #06989A
URxvt.color7: #D3D7CF
URxvt.color8: #555753
URxvt.color9: #EF2929
URxvt.color10: #8AE234
URxvt.color11: #FCE94F
URxvt.color12: #729FCF
URxvt.color13: #AD7FA8
URxvt.color14: #34E2E2
URxvt.color15: #EEEEEC
 
URxvt.transparent: true
URxvt.shading: 3
URxvt.fading: 5
 
URxvt.scrollBar: false
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true

URxvt*letterSpace: -1
URxvt*font: -1
URxvt*font: xft:Envy Code R for Powerline:pixelsize=12
